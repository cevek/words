{
  "env": {
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "blockBindings": true
  },
  "globals": {
    "window": true,
    "document": true,
    "document": true,
    "navigator": true,
    "setTimeout": true,
    "location": true,
    "history": true,
    "XMLHttpRequest": true,
    "localStorage": true,
    "sessionStorage": true,
    "io": true,
    "VK": true,
    "console": true,
    "React": true,
    "module": true,
    "exports": true,
    "__dirname": true,
    "process": true,
    "require": true
  },
  "rules": {
    "strict": 0,
    "no-var": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-undef": 2,
    "no-undef": 2,
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "func-names": 0,
    "indent": [
      2,
      4
    ],
    "comma-dangle": 0,
    "eqeqeq": 0,
    "no-const-assign": 2,
    "no-console": 0,
    "brace-style": 0,
    "no-new": 0,
    "spaced-comment": 0,
    "no-use-before-define": 0,
    "padded-blocks": 0,
    "space-before-blocks": 0,
    "missing-semicolon": 0
  }
}
